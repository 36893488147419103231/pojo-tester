{
  "plugins" : ["anchors",
               "toggle-chapters",
               "sharing",
               "editlink",
               "github-buttons",
               "share",
               "sitemap", 
               "nocache", 
               "git-author",
               "back-to-top-button",
	       "disqus",
               "advanced-emoji",
               "ga",
               "github"],
  "pluginsConfig": {
      "git-author": {
          "position": "top",
          "modifyTpl": "Last modified by {user} {timeStamp}",
          "createTpl": "Created by {user} {timeStamp}",
          "timeStampFormat": "YYYY-MM-DD HH:mm:ss"
      },
      "ga": {
          "token": "UA-84695813-1"
      },
      "sitemap": {
          "hostname": "https://pojo.pl/"
      },
      "github": {
          "url": "https://github.com/sta-szek/pojo-tester"
      },
      "editlink": {
        "base": "https://github.com/sta-szek/pojo-tester-pages/edit/master",
        "label": "Edit This Page",
        "multilingual": false
      },
      "github-buttons": {
        "repo": "sta-szek/pojo-tester",
        "types": [
          "star",
          "watch",
          "fork",
          "follow"
        ]
      },
      "disqus":{
      	"shortName": "pojo"
      }
   
  }
}
